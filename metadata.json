{
  "name": "cirrax-autoconfigmail",
  "version": "1.2.1",
  "author": "Cirrax GmbH",
  "summary": "A module to create a vhost for autoconfigure/autodiscover email services",
  "license": "GPL-3.0",
  "source": "https://github.com/cirrax/puppet-autoconfigmail",
  "project_page": "https://github.com/cirrax/puppet-autoconfigmail",
  "issues_url": "https://github.com/cirrax/puppet-autoconfigmail/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.2.0 < 9.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 2.0.0 < 8.0.0"
    },
    {
      "name": "puppetlabs/apache",
      "version_requirement": ">= 2.0.0 < 10.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9",
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "20.04",
        "22.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "openvox",
      "version_requirement": ">=4.0.0 < 8.0.0"
    }
  ],
  "tags": [
    "mail",
    "email",
    "autoconfig",
    "autoconfigure"
  ]
}
